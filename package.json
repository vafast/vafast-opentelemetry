{
	"name": "@vafast/opentelemetry",
	"version": "0.0.1",
	"description": "Plugin for Vafast for OpenTelemetry integration",
	"license": "MIT",
	"scripts": {
		"dev": "bun run --watch example/index.ts",
		"test": "bun test",
		"build": "bun build.ts",
		"release": "npm run build && npm run test && npm publish --access public"
	},
	"peerDependencies": {
		"vafast": ">= 0.1.12"
	},
	"devDependencies": {
		"@types/bun": "1.1.14",
		"eslint": "9.39.2",
		"tsup": "^8.1.0",
		"typescript": "^5.5.3",
		"vafast": "^0.1.12"
	},
	"main": "./dist/cjs/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/cjs/index.js"
		}
	},
	"keywords": [
		"vafast",
		"opentelemetry",
		"telemetry"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/vafastjs/vafast-opentelemetry"
	},
	"author": {
		"name": "saltyAom",
		"url": "https://github.com/SaltyAom",
		"email": "saltyaom@gmail.com"
	},
	"homepage": "https://github.com/vafastjs/vafast-opentelemetry",
	"bugs": "https://github.com/vafastjs/vafast-opentelemetry/issues",
	"dependencies": {
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/sdk-node": "^0.203.0",
		"vafast": ">= 0.1.12"
	}
}
